<div class="container">
  <div class="row">
    <div class="offset4 span4 well">
      <%= form_tag password_path, :method => :post do %>
        <p>
          <%= label_tag :email %>
          <%= text_field_tag :email, params[:email] %>
        <p>
        <p><%= submit_tag("Reset Password", :class => "btn btn-primary") %></p>
      <% end %>
    </div>
  </div>
</div>`
